{
    "__project_type": "generic",
    "project_name": "3d-model",
    "__project_slug": "{{ cookiecutter.project_name | lower | replace('-', '_') | replace('.', '_') }}",

    "full_name": "Mike Salvatore",
    "email": "mike.s.salvatore@gmail.com",

    "url": "https://github.com/mssalvatore/{{ cookiecutter.project_name }}",

    "__year": "{% now 'utc', '%Y' %}",
    "license": ["GPLv3", "MIT", "Apache", null],
    "versioning_scheme": ["semver", "PEP440"],

